{
  "title": "webpack-code-splitting",
  "name": "webpack-code-splitting",
  "scripts": {
    "postinstall": "find ./packages/* -name package.json -maxdepth 2 -execdir yarn \\;",
    "clean": "find . -name package.json -execdir rm -fr node_modules npm-debug.log yarn.lock \\;",
    "nuke": "npm run clean",
    "build": "yarn; rm -fr dist && webpack --progress --bail --config webpack.production.config.babel.js",
    "dev": "yarn; rm -fr dist && webpack-dev-server --open --hot --progress --config webpack.dev.config.babel.js"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "img-loader": "^2.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-chunk-hash": "^0.4.0"
  }
}
